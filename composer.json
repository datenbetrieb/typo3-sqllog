{
    "name": "datenbetrieb/sqllog",
    "type": "typo3-cms-extension",
    "description": "A small TYPO3 extension that extends Database/Conneection with SQL-Logging",
    "license": [
	    "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Peter Niederlag",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5 || ^10.4 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "Datenbetrieb\\Sqllog\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datenbetrieb\\Sqllog\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/sqllog": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "sqllog"
        }
    }
}
